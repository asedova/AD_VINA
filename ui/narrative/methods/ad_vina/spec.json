{
    "ver": "0.0.1",
    "authors": [
        "suminw, asedova, tgu2"
    ],
    "contact": ["help@kbase.us", "sumin.w.wang@gmail.com"],
    "categories": ["active", "structural_genomics"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameter-groups": [
        {
            "id": "search_space",
            "parameters": [
                "center_x",
                "center_y",
                "center_z",
                "size_x",
                "size_y",
                "size_z"
            ],
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "with_border": false
        }
    ],
    "parameters": [ 
        {
            "id": "pdb_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "valid_ws_types" : [
                  "KBaseStructure.ModelProteinStructure",
                  "KBaseStructure.ExperimentalProteinStructure"
               ]
            }
        },
        {
            "id": "ligand_list_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "valid_ws_types" : [
                  "KBaseBiochem.CompoundSet"
               ]
            }
        },
        {
            "id": "center_x",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : -1000,
               "max_int" : 1000
            }
        },
        {
            "id": "center_y",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : -1000,
               "max_int" : 1000
            }
        },
        {
            "id": "center_z",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : -1000,
               "max_int" : 1000
            }
        },
        {
            "id": "size_x",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : 0,
               "max_int" : 10000
            }
        },
        {
            "id": "size_y",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : 0,
               "max_int" : 10000
            }
        },
        {
            "id": "size_z",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "float",
               "min_int" : 0,
               "max_int" : 10000
            }
        },
        {
            "id": "seed",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : -2147483648,
               "max_int" : 2147483647
            }
        },
        {
            "id": "exhaustiveness",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "20" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : 1,
               "max_int" : 200
            }
        },
        {
            "id": "num_modes",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "1000" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : 0,
               "max_int" : 10000
            }
        },
        {
            "id": "energy_range",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options" : {
               "validate_as" : "int",
               "min_int" : 0,
               "max_int" : 10000
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "AD_VINA",
            "method": "ad_vina",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },
                {
                    "input_parameter": "pdb_ref",
                    "target_property": "pdb_ref",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "ligand_list_ref",
                    "target_property": "ligand_list_ref",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "search_space",
                    "target_property": "search_space"
                },
                {
                    "input_parameter": "num_modes",
                    "target_property": "num_modes"
                },
                {
                    "input_parameter": "seed",
                    "target_property": "seed"
                },
                {
                    "input_parameter": "energy_range",
                    "target_property": "energy_range"
                },
                {
                    "input_parameter": "exhaustiveness",
                    "target_property": "exhaustiveness"
                }      
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
